{
  "build": {
    "core": "esp8266",
    "extra_flags": "-DESP8266 -DARDUINO_ARCH_ESP8266 -DARDUINO_ESP8266_ESP01",
    "f_cpu": "160000000L",
    "f_flash": "40000000L",
    "flash_mode": "qio",
    "ldscript": "eagle.flash.1m128.ld",
    "mcu": "esp8266",
    "variant": "generic"
  },
  "connectivity": [
    "wifi"
  ],
  "frameworks": [
    "arduino",
    "esp8266-rtos-sdk",
    "esp8266-nonos-sdk"
  ],
  "name": "Espressif ESP-PRO Robodyn 1M 64/128k spiffs",
  "upload": {
    "maximum_ram_size": 81920,
    "maximum_size": 1048576,
    "require_upload_port": true,
    "resetmethod": "ck",
    "speed": 115200
  },
  "url": "http://www.esp8266.com/wiki/doku.php?id=esp8266-module-family",
  "vendor": "Espressif"
}
